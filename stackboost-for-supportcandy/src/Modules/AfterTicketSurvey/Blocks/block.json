{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "stackboost/after-ticket-survey",
    "version": "1.0.0",
    "title": "After Ticket Survey",
    "category": "widgets",
    "icon": "clipboard",
    "description": "Displays the After Ticket Survey form.",
    "supports": {
        "html": false,
        "color": {
            "background": true,
            "text": true,
            "link": true
        },
        "typography": {
            "fontSize": true
        },
        "spacing": {
            "margin": true,
            "padding": true
        },
        "borders": {
            "radius": true,
            "style": true,
            "width": true,
            "color": true
        }
    },
    "attributes": {
        "formTitle": {
            "type": "string",
            "default": ""
        },
        "introText": {
            "type": "string",
            "default": "Your feedback is important to us. Please take a moment to complete this survey."
        },
        "successMessage": {
            "type": "string",
            "default": "Thank you for completing our survey! Your feedback is invaluable and helps us improve our services."
        },
        "submitButtonText": {
            "type": "string",
            "default": "Submit Survey"
        },
        "redirectUrl": {
            "type": "string",
            "default": ""
        },
        "layout": {
            "type": "string",
            "default": "list"
        }
    },
    "textdomain": "stackboost-for-supportcandy",
    "editorScript": "file:./index.js",
    "style": "stackboost-ats-frontend"
}
